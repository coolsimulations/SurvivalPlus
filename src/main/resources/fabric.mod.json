{
  "schemaVersion": 1,
  "id": "survivalplus",
  "version": "${version}",

  "name": "SurvivalPlus",
  "description": "SurvivalPlus enhances the vanilla survival experience of Minecraft whilst interacting with other mods to create seamless gameplay.",
  "authors": [
    "coolsim"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/survivalplus-fabric",
    "sources": "http://github.com/coolsimulations/survivalplus/",
    "issues": "http://github.com/coolsimulations/survivalplus/issues/"
  },

  "license": "All Rights Reserved",
  "icon": "logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.coolsimulations.SurvivalPlus.core.SurvivalPlus"
    ],
    "modmenu": [
      "net.coolsimulations.SurvivalPlus.core.config.SPConfigGUI"
    ],
    "rei_plugins": [
      "net.coolsimulations.SurvivalPlus.core.util.SurvivalPlusREI"
    ]
  },
  
  "mixins": [
    "survivalplus.mixins.json"
    ],
    
  "depends": {
    "fabricloader": ">=0.10.8",
    "fabric": "*",
    "minecraft": "1.14.4"
  },
  
  "suggests": {
    "cloth-config2": ">=1.8",
    "roughlyenoughitems": ">=3.3.0",
    "refinedmachinery": ">=1.2.1",
    "blockus": ">=1.2.0",
    "bambootweaks": ">=1.1.0",
    "bambooeverything": ">=1.4.0",
    "bamboomod": ">=0.3.0"
  }
}